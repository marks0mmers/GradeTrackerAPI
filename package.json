{
  "name": "grade-tracker-api",
  "version": "1.0.0",
  "description": "API for the Grade Tracker App",
  "main": "src/index.ts",
  "engines": {
    "node": "10.13.0"
  },
  "scripts": {
    "start-dev": "NODE_ENV=development node_modules/.bin/nodemon --exec 'yarn run lint && node_modules/.bin/ts-node src/index.ts'",
    "start": "NODE_ENV=production node_modules/.bin/nodemon --exec 'yarn run lint && node_modules/.bin/ts-node src/index.ts'",
    "debug": "NODE_ENV=development node_modules/.bin/nodemon --exec 'yarn run lint && node_modules/.bin/ts-node -d src/index.ts'",
    "build": "cd client && yarn install && yarn build",
    "prod": "NODE_ENV=production node_modules/.bin/ts-node src/index.ts",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts' -t verbose --force",
    "test": "node_modules/.bin/ts-node node_modules/.bin/tape './test/**/*.spec.ts' | tap-spec",
    "heroku-postbuild": "yarn prod"
  },
  "author": "Mark Sommers",
  "license": "MIT",
  "dependencies": {
    "@types/bluebird": "^3.5.25",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/express-jwt": "0.0.41",
    "@types/express-session": "^1.15.11",
    "@types/inversify": "^2.0.33",
    "@types/lodash": "^4.14.120",
    "@types/mongodb": "^3.1.19",
    "@types/mongoose": "^5.3.12",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.11",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typeorm": "^0.2.11",
    "typescript": "^3.2.2",
    "winston": "2.4.4"
  },
  "heroku-run-build-script": true
}
